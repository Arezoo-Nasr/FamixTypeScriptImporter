{
  "name": "ts2famix",
  "version": "0.1.0",
  "description": "Examples of the TypeScript compiler API usage",
  "main": "dist/ts2famix-cli.js",
  "scripts": {
    "dev": "ts-node src/ts2famix-cli.ts",
    "debug": "node --inspect-brk node_modules/.bin/ts-node",
    "build": "tsc",
    "test": "jest --colors",
    "local": "sudo npm i -g && ts2famix",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "bin": {
    "ts2famix": "dist/ts2famix-cli.js"
  },
  "keywords": [],
  "author": "",
  "nodemonConfig": {
    "ignore": [
      "dist/*"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "jest": "^27.2.2",
    "jest-extended": "^0.11.5",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "ts-morph": "^12.0.0",
    "yargs": "^17.2.1"
  }
}
